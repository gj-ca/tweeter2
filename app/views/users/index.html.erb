<h1>All users</h1>
<table>
    <tr>
        <th>Username</th>
        <th>Password</th>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.password %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
            <td><%= link_to "Edit", edit_user_path(user.id) %></td>
            <td><%= link_to "Destroy", "/users/#{user.id}", method: "delete" %></td>
        </tr>
    <% end %>
</table>
<%= link_to "New User", users_new_path %>