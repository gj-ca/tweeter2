<h1>Edit User</h1>
<%= form_with(model: @user, local: true) do |form| %>
    <div>
        <%= form.label :username %>
        <%= form.text_field :username %>
    </div>
    <div>
        <%= form.label :password %>
        <%= form.text_field :password %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>